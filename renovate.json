{
  "extends": ["config:js-app", ":semanticCommits"],
  "labels": ["renovate"],
  "automerge": true,
  "prConcurrentLimit": 10,
  "packageRules": [
    {
      "packagePatterns": "react",
      "groupName": ["react"]
    },
    {
      "packagePatterns": "stylelint",
      "groupName": ["stylelint"]
    },
    {
      "packagePatterns": "graphql",
      "groupName": ["graphql"]
    }
  ]
}
